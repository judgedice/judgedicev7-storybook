{
  "name": "judgedicev7",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "lib-dist/index.js",
  "types": "lib-dist/index.d.ts",
  "files": [
    "lib-dist"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "tsc",
    "build:lib": "rm -rf lib-dist && tsc --project tsconfig.lib.json && mkdir -p lib-dist/stories && cp -r src/stories/**/*.css lib-dist/stories/",
    "build:storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "prepare": "yarn build:lib"
  },
  "dependencies": {
    "judgedicev7": "file:../judgedicev7-storybook",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "style-loader": "^3.3.4",
    "css-loader": "^6.10.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.0",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "^9.0.6",
    "@storybook/addon-docs": "^9.0.6",
    "@storybook/addon-onboarding": "^9.0.6",
    "@storybook/addon-vitest": "^9.0.6",
    "@storybook/nextjs-vite": "9.0.6",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.2.2",
    "@vitest/coverage-v8": "^3.2.2",
    "chromatic": "^12.1.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-storybook": "^9.0.6",
    "playwright": "^1.52.0",
    "storybook": "^9.0.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "tsc-alias": "^1.8.8",
    "vitest": "^3.2.2",
    "typescript-plugin-css-modules": "^5.0.1"
  }
}
